# Example environment variables
#
# DATABASE_URL: Connection string for your database
# SECRET_KEY: Secret key for cryptographic signing (change in production!)
#    DEBUG: Set to True for development, False for production
# WORDPRESS_DB_HOST: Host for the WordPress database
# WORDPRESS_DB_NAME: Name of the WordPress database
# WORDPRESS_DB_USER: Username for the WordPress database
# WORDPRESS_DB_PASSWORD: Password for the WordPress database
# SUPABASE_API_URL: URL for the Supabase API
# SUPABASE_SERVICE_ROLE_KEY: Service role key for Supabase API access



DATABASE_URL=postgres://user:password@localhost:5432/dbname
SECRET_KEY=your-secret-key
DEBUG=True
WORDPRESS_DB_HOST=your-mysql-host
WORDPRESS_DB_NAME=your-db-name
WORDPRESS_DB_USER=your-db-user
WORDPRESS_DB_PASSWORD=your-db-password
SUPABASE_API_URL=https://your-project.supabase.co/rest/v1/wp-sync
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key